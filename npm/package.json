{
  "name": "@tailjs/monorepo",
  "description": "Monorepo for tail.js",
  "license": "LGPL3",
  "private": true,
  "author": "RelevantEdge (https://www.relevant-edge.com)",
  "homepage": "https://github.com/relevantedge/tailjs",
  "scripts": {
    "check": "pnpm -r run --stream check",
    "build": "pnpm -r run --stream build",
    "schema": "pnpm --filter @tailjs/types schema",
    "build:watch": "pnpm --workspace-concurrency=2 -r --parallel run --stream build:watch",
    "build:engine": "pnpm --workspace-concurrency=2 -r --filter=\"@tailjs/engine...\" --filter=\"@tailjs/client...\" run --stream build",
    "build:engine:watch": "pnpm --workspace-concurrency=2 -r --parallel --filter=\"@tailjs/engine...\" --filter=\"@tailjs/client...\" --filter=\"@tailjs/maxmind\" run --stream build:watch --ext",
    "build:jss:watch": "pnpm --workspace-concurrency=2 -r run --stream --filter=\"@tailjs/sitecore-jss...\" build:watch",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "devDependencies": {
    "@pnpm/find-workspace-dir": "^6.0.3",
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-commonjs": "^25.0.8",
