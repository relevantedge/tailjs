{
  "name": "@tailjs/types",
  "description": "Default type definitions for tailjs.",
  "license": "LGPL3",
  "version": "0.17.24",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "schema": "tsx ../../../build/export-schema.ts",
    "check": "tsc --noEmit",
    "build": "tsx ../../../build/rollup.ts && tsx ../../../build/export-schema.ts",
    "build:watch": "nodemon --watch src --watch ../../../build/export-schema.ts --ext ts --exec \"pnpm run build\""
  },
  "devDependencies": {
    "typedoc": "^0.24.8",
    "typedoc-plugin-expand-object-like-types": "^0.1.2",
    "typedoc-plugin-markdown": "^3.17.1"
  },
  "publishConfig": {
    "directory": "dist"
  },
  "dependencies": {
    "@tailjs/util": "workspace:*"
  }
}
