{
  "name": "@tailjs/transport",
  "description": "The wire format used by tail.js.",
  "license": "LGPL3",
  "version": "0.17.24",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "type": "module",
  "scripts": {
    "check": "tsc --noEmit",
    "build": "tsx ../../../build/rollup.ts",
    "build:watch": "pnpm run build -w -t"
  },
  "publishConfig": {
    "directory": "dist"
  },
  "dependencies": {
    "@tailjs/util": "^0.17.24",
    "@ygoe/msgpack": "github:nielskuhnel/msgpack.js#feature/big-float-fix"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "bin": {}
}