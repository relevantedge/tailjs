{
  "name": "@tailjs/react",
  "description": "tail.js preact/react integration.",
  "license": "LGPL3",
  "version": "0.17.24",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "scripts": {
    "check": "tsc --noEmit",
    "build": "tsx ../../../build/rollup.ts",
    "build:watch": "pnpm run build -w -t"
  },
  "dependencies": {
    "@tailjs/client": "workspace:*",
    "@tailjs/types": "workspace:*",
    "@tailjs/util": "workspace:*",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "publishConfig": {
    "directory": "dist"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "preact": "^10.22.1",
    "preact-render-to-string": "^6.5.6"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  }
