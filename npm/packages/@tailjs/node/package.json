{
  "name": "@tailjs/node",
  "description": "A node.js native engine host for tail.js.",
  "license": "LGPL3",
  "version": "0.17.24",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "type": "module",
  "scripts": {
    "check": "tsc --noEmit",
    "build": "tsx ../../../build/rollup.ts",
    "build:watch": "pnpm run build -w",
    "serve": "tsx ../../../build/rollup.ts -c cli",
    "serve:watch": "pnpm run serve -w"
  },
  "bin": {
    "tailjs": "src/cli.ts"
  },
  "publishConfig": {
    "directory": "dist"
  },
  "dependencies": {
    "@tailjs/engine": "workspace:*",
    "@tailjs/json-schema": "workspace:*",
    "@tailjs/transport": "workspace:*",
    "@tailjs/types": "workspace:^0.17.24",
    "@tailjs/util": "workspace:*",
    "request-ip": "^3.3.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^18.19.33",
    "@types/request-ip": "^0.0.38"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  }
}