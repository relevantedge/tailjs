{
  "name": "@tailjs/sitecore-jss",
  "description": "tail.js integration for Sitecore XM Cloud/JSS.",
  "license": "LGPL3",
  "scripts": {
    "check": "tsc --noEmit",
    "build": "tsx ../../../build/rollup.ts",
    "build:watch": "pnpm run build -w"
  },
  "dependencies": {
    "@sitecore-jss/sitecore-jss": "^21.7.1",
    "@sitecore-jss/sitecore-jss-nextjs": "^21.7.1",
    "@tailjs/client": "workspace:*",
    "@tailjs/engine": "workspace:*",
    "@tailjs/next": "workspace:*",
    "@tailjs/react": "workspace:*",
    "@tailjs/types": "workspace:*",
    "@types/request-ip": "^0.0.38",
    "fs-extra": "^11.2.0",
    "next": "^14.2.3",
    "react": "^18.3.1",
    "request-ip": "^3.3.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^18.19.33",
    "@types/react": "^18.3.3"
  },
  "peerDependencies": {
    "next": ">=13.0.0",
    "react": "^18.3.1"
  },
  "publishConfig": {
    "directory": "dist"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  }
}
