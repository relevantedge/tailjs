{
  "name": "@tailjs/next",
  "description": "tail.js NextJS integration.",
  "license": "LGPL3",
  "version": "0.17.24",
  "scripts": {
    "check": "tsc --noEmit",
    "build": "tsx ../../../build/rollup.ts --preserve-modules",
    "build:watch": "pnpm run build -w"
  },
  "dependencies": {
    "@tailjs/engine": "workspace:^0.17.24",
    "@tailjs/node": "workspace:*",
    "@tailjs/react": "workspace:*"
  },
  "devDependencies": {
    "@types/node": ">=18.19.33",
    "@types/react": ">=18.3.3"
  },
  "publishConfig": {
    "directory": "dist"
  },
  "peerDependencies": {
    "next": ">=13.0.0",
    "react": ">=18.3.1"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  }
}
