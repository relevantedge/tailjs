{
  "name": "@tailjs/next",
  "description": "tail.js NextJS integration.",
  "license": "LGPL3",
  "version": "0.17.24",
  "scripts": {
    "check": "tsc --noEmit",
    "build": "tsx ../../../build/rollup.ts",
    "build:watch": "pnpm run build -w",
    "postinstall": "npx tailjs-configure-next"
  },
  "dependencies": {
    "@tailjs/engine": "^0.17.24",
    "@tailjs/node": "^0.17.24",
    "@tailjs/react": "^0.17.24"
  },
  "devDependencies": {
    "@types/node": ">=18.19.33",
    "@types/react": ">=18.3.3"
  },
  "bin": {
    "tailjs-init-next": "./dist/cli/tailjs-init-next.cjs"
  },
  "publishConfig": {
    "directory": "dist"
  },
  "peerDependencies": {
    "next": ">=13.0.0",
    "react": ">=18.3.1"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    },
    "./server": {
      "import": {
        "types": "./dist/server/index.d.ts",
        "default": "./dist/server/index.mjs"
      },
      "require": {
        "types": "./dist/server/index.d.ts",
        "default": "./dist/server/index.cjs"
      }
    }
  }
}