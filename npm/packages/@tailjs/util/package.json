{
  "name": "@tailjs/util",
  "description": "Common utilities for all @tailjs packages.",
  "license": "LGPL3",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "type": "module",
  "scripts": {
    "check": "tsc --noEmit",
    "build": "tsx ../../../build/rollup.ts",
    "build:watch": "pnpm run build -w"
  },
  "publishConfig": {
    "directory": "dist"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  }
}
